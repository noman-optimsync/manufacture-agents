{
  "name": "@mastra/libsql",
  "version": "0.16.3",
  "description": "Libsql provider for Mastra - includes both vector and db storage capabilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@libsql/client": "^0.15.15"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^20.19.0",
    "eslint": "^9.37.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@internal/storage-test-utils": "0.0.60",
    "@mastra/core": "0.24.6",
    "@internal/lint": "0.0.64",
    "@internal/types-builder": "0.0.39"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.21.2-0 <0.25.0-0"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "stores/libsql"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "publishConfig": {
    "access": "public",
    "publish-branch": [
      "main",
      "0.x"
    ]
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "tsup --watch --silent --config tsup.config.ts",
    "test": "vitest run",
    "lint": "eslint ."
  }
}